<%= form_for(@entry) do |f| %>
	<ul>
		<% @entry.errors.full_messages.each do |error| %>
			<li><%= error %></li>
		<% end %>
	</ul>
	<p>
		<%= f.label :entry_type %><br />
		<%= f.text_field :entry_type %>
	</p>
	<p>
		<%= f.label :input %><br />
		<%= f.text_area :input %>
	</p>
	<p>
		<%= f.submit %>
	</p>
<% end %>