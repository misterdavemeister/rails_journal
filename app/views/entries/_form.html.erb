<%= form_for(@entry, html: {multipart: true}) do |f| %>
	<ul>
		<% @entry.errors.full_messages.each do |error| %>
			<li><%= error %></li>
		<% end %>
	</ul>
	<p>
		<%= f.label :entry_type %><br/>
		<%= f.select :entry_type, [["Food", "Food"], ["Symptom", "Symptom"]] %>
		<!--<%= f.label :entry_type %><br />
		<%= f.text_field :entry_type %>-->	
	</p>
	<p>
		<%= f.label :input %><br/>
		<%= f.text_area :input %>
	</p>
	<p>
		<%= f.label :tag_list %><br/>
		<%= f.text_field :tag_list %>
	</p>
	<p>
		<% if @entry.image.exists? %>
			<%= image_tag @entry.image.url(:thumb) %>
		<% end %>
		<%= f.label :image, "Attach New Image" %><br/>
		<%= f.file_field :image %>
		<!--TODO: checkbox example: 
			<%= check_box_tag(:pet_dog) %>
			<%= label_tag(:pet_dog, "I own a dog") %>
			<%= check_box_tag(:pet_cat) %>
			<%= label_tag(:pet_cat, "I own a cat") %>
		-->
	</p>
	<p>
		<%= f.submit %>
	</p>
<% end %>